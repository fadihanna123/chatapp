{
  "name": "server",
  "version": "1.0.24",
  "lastUpgrade": "20260109",
  "scripts": {
    "setup": "yarn --emoji --silent --ignore-scripts --immutable",
    "start": "node dist/server",
    "dev": "ts-node-dev --poll -r tsconfig-paths/register src/server",
    "build": "esbuild ./src/server.ts --bundle --minify --packages=external --sourcemap --outdir=./build --platform=node",
    "lint": "eslint && tsc --noEmit && yarn type-check",
    "format": "prettier ./ -w",
    "type-check": "tsc --noEmit",
    "clear": "rimraf ./node_modules ./build"
  },
  "license": "MIT",
  "dependencies": {
    "dotenv": "17.2.3",
    "mysql2": "^3.16.0",
    "socket.io": "4.8.3"
  },
  "devDependencies": {
    "@eslint/js": "9.39.2",
    "esbuild": "^0.27.2",
    "eslint": "9.39.2",
    "globals": "17.0.0",
    "prettier": "3.7.4",
    "ts-node-dev": "2.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.52.0"
  }
}
